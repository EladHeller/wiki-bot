    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames":false,
        "strictNullChecks": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules"
    ]
}
