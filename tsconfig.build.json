{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames":false,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "outDir": "dist"
    },
    "include": ["bot"],
    "exclude": ["bot/__tests__"]
}
