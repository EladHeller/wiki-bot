{
  "name": "wiki-bot",
  "version": "1.0.0",
  "description": "wiki-bot",
  "main": "dist/index.js",
  "scripts": {
    "lint:test": "eslint ./**/*.ts ./send-email/**/*.mjs ./xmlbot/**/*.js --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage",
    "build": "rm -rf dist && tsc",
    "update-env": "tsx ./build/update-env.ts",
    "test:ci": "npm run type-check && npm test && npm run lint:test",
    "rollback": "tsx bot/scripts/rollback.ts",
    "experiments": "tsx bot/experiments/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EladHeller/wiki-bot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EladHeller/wiki-bot/issues"
  },
  "homepage": "https://github.com/EladHeller/wiki-bot#readme",
  "dependencies": {
    "@hebcal/core": "^5.0.7",
    "axios": "^1.6.2",
    "axios-cookiejar-support": "^4.0.7",
    "dotenv": "^16.3.1",
    "jsdom": "^23.0.1",
    "openai": "^4.26.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "^3.484.0",
    "@aws-sdk/client-lambda": "^3.398.0",
    "@aws-sdk/client-s3": "^3.462.0",
    "@aws-sdk/client-ses": "^3.431.0",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.11.24",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "zx": "^7.2.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "BaseWikiApi.ts",
      "NewWikiApi.ts",
      "wikiLogin.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
